	</s:states>
	
	<s:Rect width="1" height="100%">  
		<s:fill>  
			<s:LinearGradient rotation="90">  
				<s:entries>  
					<s:GradientEntry color.up="#000000" alpha.up="0.2" color.over="#277bc2" color.down="#277bc2" />  
					<s:GradientEntry color.up="#000000" alpha="0" ratio.up="0.7" color.over="#277bc2" color.down="#277bc2" />
				</s:entries>  
			</s:LinearGradient>  
		</s:fill>  
	</s:Rect> 
	<s:Rect width="1" height="100%" left="1">  
		<s:fill>  
			<s:LinearGradient rotation="90">  
				<s:entries>  
					<s:GradientEntry color.up="#cccccc" alpha.up="0.3" color.over="#52bbe0" color.down="#3199bf" />  
					<s:GradientEntry color.up="#ffffff" alpha="0" ratio.up="0.7" color.over="#52bbe0" color.down="#3199bf" />
				</s:entries>  
			</s:LinearGradient>  
		</s:fill>  
	</s:Rect> 
	<s:Rect width="100%" height="100%" left="2">  
		<s:fill>  
			<s:LinearGradient rotation="90">  
				<s:entries>  
					<s:GradientEntry color.up="#ffffff" alpha.up="0.05" color.over="#2aa4c4" color.down="#0d86b5" />  
					<s:GradientEntry color.up="#ffffff" alpha="0" ratio.up="0.7" color.over="#2aa4c4" color.down="#0d86b5" />
				</s:entries>  
			</s:LinearGradient>  
		</s:fill>  
	</s:Rect>
	
	<s:Path data="M0 0 10 0 10 2 0 2Z M0 2 1 2 1 7 0 7Z M9 2 10 2 10 7 9 7Z M0 7 10 7 10 8 0 8Z" left="8" top="4">
		<s:fill>
			<s:SolidColor color="#e4d8cf" />
		</s:fill>
	</s:Path>
	
	
	<!-- SkinParts
	name=labelDisplay, type=spark.components.supportClasses.TextBase, required=false
	-->
</s:Skin>
