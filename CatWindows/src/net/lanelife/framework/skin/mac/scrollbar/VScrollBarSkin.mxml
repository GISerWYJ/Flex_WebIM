<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.VScrollBar")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="inactive" />
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
	<s:Button id="track" top="16" bottom="16" focusEnabled="false"
			  skinClass="net.lanelife.framework.skin.mac.scrollbar.VScrollBarTrackSkin"/>
	
	<s:Button id="thumb" top="16" bottom="16"
			  focusEnabled="false" visible.inactive="false"
			  skinClass="net.lanelife.framework.skin.mac.scrollbar.VScrollBarThumbSkin" />
	
	<s:Button id="decrementButton" top="0" focusEnabled="false"
			  skinClass="net.lanelife.framework.skin.mac.scrollbar.ScrollBarUpButtonSkin"
			  enabled.inactive="false"/>
	
	<s:Button id="incrementButton" bottom="0" enabled.inactive="false"
			  focusEnabled="false" 
			  skinClass="net.lanelife.framework.skin.mac.scrollbar.ScrollBarDownButtonSkin" />
	
	<!-- SkinParts
	name=incrementButton, type=spark.components.Button, required=false
	name=track, type=spark.components.Button, required=false
	name=thumb, type=spark.components.Button, required=false
	name=decrementButton, type=spark.components.Button, required=false
	-->
</s:Skin>
