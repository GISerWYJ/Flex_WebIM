<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="up" />
	</s:states>
	
	<s:Image left="0" top="0" width="100%" height="100%" includeIn="over, down" scaleMode="stretch" source="@Embed(source='track_skin.png')" />
	<s:Image left="0" top="0" width="100%" height="100%" includeIn="disabled, up" scaleMode="stretch" source="@Embed(source='track_skin.png')" />
	
</s:Skin>
