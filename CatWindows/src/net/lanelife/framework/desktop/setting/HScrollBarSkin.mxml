<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.HScrollBar")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="inactive" />
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
	<s:Rect top="0" bottom="0" left="0" right="0" minWidth="10" minHeight="10">
		<s:fill>
			<s:SolidColor color="#dbdee1" />
		</s:fill>
	</s:Rect>
	
	<s:Button id="track" left="0" right="0"
			  focusEnabled="false"
			  skinClass="net.lanelife.framework.desktop.setting.HScrollBarTrackSkin" />
	
	<s:Button id="thumb" 
			  focusEnabled="false" visible.inactive="false"
			  skinClass="net.lanelife.framework.desktop.setting.HScrollBarThumbSkin" />
	
	
	<!-- SkinParts
	name=incrementButton, type=spark.components.Button, required=false
	name=track, type=spark.components.Button, required=false
	name=thumb, type=spark.components.Button, required=false
	name=decrementButton, type=spark.components.Button, required=false
	-->
</s:Skin>
